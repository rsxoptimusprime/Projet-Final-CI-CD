[Unit]
Description=Artifactory service
After=network.target

[Service]
Type=forking
LimitNOFILE=65536
Environment=JAVA_HOME=/opt/jdk1.8
Environment=JRE_HOME=/opt/jdk1.8/jre
ExecStart=/opt/artifactory/artifactory-oss-6.3.3/bin/artifactory.sh start
ExecStop=/opt/artifactory/artifactory-oss-6.3.3/bin/artifactory.sh stop
User=artifactory
Restart=on-abort

[Install]
WantedBy=multi-user.target
